%head
  <link href='http://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
%div#title Sign Up

%div#sign-up-form
  = form_for resource, as: resource_name, url: registration_path(resource_name) do |f|

    %div.sign-up-field
    = label :members, :email, 'Email'
    %br
    = text_field :members, :email, :placeholder: "john@example.com", :value => "#{@email}"

    %div.sign-up-field
      = label :members, :password, 'Password'
